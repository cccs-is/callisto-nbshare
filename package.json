{
  "name": "callisto-nbshare",
  "version": "0.1.0",
  "description": "sharing notebooks on Callisto",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/cccs-is/callisto-nbshare",
  "bugs": {
    "url": "https://github.com/cccs-is/callisto-nbshare/issues"
  },
  "license": "BSD-3-Clause",
  "author": "cccs-is",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/cccs-is/callisto-nbshare.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/filebrowser": "^1.2.1",
    "@jupyterlab/notebook": "^1.2.2",
    "@lumino/algorithm": "^1.2.3",
    "@lumino/coreutils": "^1.4.2"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "kernel": [
        {
          "kernel_spec": {
            "language": "^python"
          },
          "base": {
            "name": "callisto-nbshare"
          },
          "managers": [
            "pip"
          ]
        }
      ]
    },
    "server": {
      "base": {
        "name": "callisto_nbshare"
      },
      "managers": [
        "pip"
      ]
    }
  }
}
